{
  "name": "wf-ep-js-project",
  "version": "1.0.0",
  "description": "A modular JavaScript project using GSAP, Lenis, and Swiper, bundled with Parcel.",
  "main": "dist/app.js",
  "dependencies": {
    "gsap": "^3.12.5",
    "split-type": "^0.3.4"
  },
  "devDependencies": {
    "parcel": "^2.13.3"
  },
  "scripts": {
    "dev": "parcel src/app.js",
    "build": "parcel build src/app.js --dist-dir dist --public-url ./ --no-scope-hoist --no-content-hash"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "keywords": [
    "webflow",
    "gsap",
    "parcel",
    "swiper",
    "lenis",
    "javascript"
  ],
  "author": "Dominic Hutson",
  "license": "MIT"
}
