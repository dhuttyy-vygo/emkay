{
  "name": "wf-ep-js-project",
  "version": "1.0.0",
  "description": "A modular JavaScript project using GSAP, Lenis, and Swiper, bundled with esbuild for prod.",
  "main": "dist/app.js",
  "dependencies": {
    "gsap": "^3.12.5",
    "split-type": "^0.3.4",
    "swiper": "^9.1.0"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-buddy": "^2.13.3",
    "esbuild": "^0.24.2",
    "parcel": "^2.11.0"
  },
  "scripts": {
    "dev": "esbuild src/app.js src/swiperapp.js --bundle --sourcemap --outdir=dist --serve=3000 --servedir=.",
    "dev:parcel": "parcel src/*.js --open",
    "build": "node esbuild.js"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "keywords": [
    "webflow",
    "gsap",
    "parcel",
    "swiper",
    "lenis",
    "javascript"
  ],
  "author": "Dominic Hutson",
  "license": "MIT"
}
